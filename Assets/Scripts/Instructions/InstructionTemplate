namespace PipelineDreams
{
    public class InstructionXXX : Instruction {
        public InstructionXXX(EntityDataContainer eM, Entity player, CommandsContainer pC, InstructionData data, string variant) : base(eM, player, pC, data, variant) {
        }

        public override IClockTask Operation(float startClock)
        {

            return new InstructionXXXTask();
        }
        
    }
    public abstract partial class Instruction
    {
        /// <summary>
        /// Field instruction task used above.
        /// </summary>
        protected class InstructionXXXTask : InstructionTask
        {
            protected override void OnRunStart()
            {
            }
        }
    }

}